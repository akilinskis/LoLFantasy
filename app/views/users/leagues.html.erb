<h1>My Leagues</h1>

<table>
    <thead>
    <tr>
        <th>League</th>
        <th>Team</th>
    </tr>
    </thead>

    <tbody>
    <% @teams.each do |team| %>
        <tr>
            <td><%= link_to team.league.name, team.league %></td>
            <td><%= link_to team.name, team %></td>
        </tr>
    <% end %>
    </tbody>
</table>