<h1>Listing free_agents</h1>

<table>
  <thead>
    <tr>
      <th>League</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @free_agents.each do |free_agent| %>
      <tr>
        <td><%= free_agent.league_id %></td>
        <td><%= link_to 'Show', free_agent %></td>
        <td><%= link_to 'Edit', edit_free_agent_path(free_agent) %></td>
        <td><%= link_to 'Destroy', free_agent, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Free agent', new_free_agent_path %>
